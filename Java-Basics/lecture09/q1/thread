 Runnable runnable1 = () -> {
            for(int i = 0; i < 10; ++i) {
                System.out.println("student " + i);
            }

        };
        Thread thread1 = new Thread(runnable1);
        Runnable runnable2 = () -> {
            for(int j = 0; j < 10; ++j) {
                System.out.println("teacher " + j);
            }

        };
        Thread thread2 = new Thread(runnable2);
        thread1.start();
        thread2.start();